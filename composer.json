{
  "name": "keith/extend-calendar",
  "description": "Adds a website field to each event in eddiewebb/flarum-calendar",
  "type": "flarum-extension",
  "license": "MIT",
  "authors": [
    { "name": "Keith McGinley" }
  ],
  "require": {
    "flarum/core": "^1.7"
  },
  "autoload": {
    "psr-4": {
      "Keith\\ExtendCalendar\\": "src/"
    }
  },
  "extra": {
    "flarum-extension": {
      "title": "Extend Calendar (Website Field)",
      "category": "feature",  
      "icon": {
        "name": "fas fa-calendar",
        "backgroundColor": "#4f46e5",
        "color": "#ffffff"
    },
      "frontend": {
        "forum": {
          "js": "js/dist/forum.js"
        }
      }
    }
  }
}